{
  "name": "@skyra/decorators",
  "version": "1.0.0",
  "description": "TypeScript decorators for Skyra",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/skyra-project/skyra-decorators",
  "author": "@skyra",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint --ext ts src test --fix",
    "test": "ava",
    "build": "tsc -b src",
    "watch": "tsc -b src -w"
  },
  "devDependencies": {
    "@types/ws": "^7.2.2",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "ava": "^3.4.0",
    "discord.js": "github:discordjs/discord.js#fe7df708e44e0280dfaf0f8e457b154781bb5140",
    "eslint": "^6.8.0",
    "eslint-config-bamboo": "^3.0.0",
    "klasa": "github:dirigeants/klasa#sgn",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "discord.js": "github:discordjs/discord.js#fe7df708e44e0280dfaf0f8e457b154781bb5140",
    "klasa": "github:dirigeants/klasa#sgn"
  },
  "files": [
    "dist",
    "!dist/tsconfig.tsbuildinfo"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.test.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
